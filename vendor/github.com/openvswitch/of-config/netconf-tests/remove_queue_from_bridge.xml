<?xml version="1.0"?>
<capable-switch xmlns="urn:onf:config:yang" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
  <id>openvswitch</id>
  <resources>
    <queue nc:operation="remove">
      <resource-id>my_queue</resource-id>
    </queue>
  </resources>
  <logical-switches>
    <switch>
      <id>ofc-bridge</id>
      <resources>
        <queue nc:operation="remove">my_queue</queue>
      </resources>
    </switch>
  </logical-switches>
</capable-switch>

